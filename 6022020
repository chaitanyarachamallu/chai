function tipcalculator(bill)
{
  var percentage;
  if(bill<50)
  {
      percentage = .2;
  }
  else if(bill >=50 && bill < 200)
  {
      percentage = .1;
  }
  return percentage * bill;
}
var bills =[124,48,268];
var tips=[tipcalculator(bills[0])];
var finalvalues=[bills[0]+tips[0]];
console.log(bills,tips);
